{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "target": "ES6",
    "lib": [
      // "ESNext",
    ],
    "module": "ESNext",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "noEmit": true,
    "plugins": [
      {
        "name": "typescript-eslint",
        "alwaysShowRuleInfo": true
      }
    ]
  }
}
